<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="EnableResponseValidation" value="true"/>
    <add key="EnableAutoCompensationForClockSkew"  value="true"/>
    <add key="LocalTimeOffsetMillis"  value="0"/>
  </appSettings>
  <system.diagnostics>
    <trace autoflush="true" />
    <sources>
      <source name="Thinktecture.IdentityModel" switchValue="All"/>
    </sources>
  </system.diagnostics>
</configuration>